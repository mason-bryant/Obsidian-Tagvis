{
	"name": "obsidian-tag-vis-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"dev": "tsc --noEmit --skipLibCheck --noEmitOnError false && node esbuild.config.mjs development",
		"easy-dev": "node esbuild.config.mjs development",
		"test": "jest",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"prettier": "prettier --write .",
		"watch": "tsc --watch --noEmit --skipLibCheck --noEmitOnError false"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-typescript": "^7.26.0",
		"@rollup/plugin-typescript": "^12.1.2",
		"@types/d3": "^7.4.3",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.16",
		"@types/node": "^16.18.126",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"babel-jest": "^29.7.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"d3": "^7.6.1",
		"lodash": "^4.17.21",
		"prettier": "^2.7.1"
	}
}
